<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.featuretest.demo" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.featuretest.demo.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.featuretest.demo.MyActivity"
            android:label="@string/title_activity_my" >
        </activity>
        <activity
            android:name="com.example.featuretest.demo.communication.ComActivity"
            android:label="@string/title_activity_com" >
        </activity>
        <activity
            android:name="com.example.featuretest.demo.listviewholder.MultiListActivity"
            android:label="@string/title_activity_multi_list" >
        </activity>
        <activity
            android:name="com.example.featuretest.demo.listviewholder.BackActivity"
            android:label="@string/title_activity_back"
            android:theme="@style/Transparent" >
        </activity>
        <activity
            android:name="com.example.featuretest.demo.asynchttp.AsyncActivity"
            android:label="@string/title_activity_async" >

        </activity>
        <activity
            android:name="com.example.featuretest.demo.navigation.drawer.NavigationActivity"
            android:label="@string/title_activity_navigation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
